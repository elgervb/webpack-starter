{
  "name": "webpack_dev",
  "version": "0.0.1",
  "description": "Site with webpack live reloading",
  "repository": {
    "type": "git",
    "url": "https://git.lukkien.com/"
  },
  "config": {
    "port": 4000,
    "dest": "./build",
    "src": "./src"
  },
  "scripts": {
    "build": "npm run build:webpack",
    "build:webpack": "babel-node -- ./node_modules/.bin/webpack --progress --colors --config webpack/config.build.js",
    "clean": "rimraf $npm_package_config_dest",
    "start": "babel-node -- ./node_modules/webpack-dev-server/bin/webpack-dev-server --inline --hot --colors --content-base=$npm_package_config_src --config ./webpack/config.dev.js --port $npm_package_config_port",
    "start:prod": "babel-node -- ./node_modules/webpack-dev-server/bin/webpack-dev-server --inline --hot --colors --content-base=$npm_package_config_dest --config ./webpack/config.build.js --port $npm_package_config_port",
    "start:remote": "$npm_package_scripts_start --host=0.0.0.0",
    "watch": "babel-node -- ./node_modules/.bin/webpack --progress --colors --config webpack/config.dev.js",
    "lint": "eslint . --ext .js --ext .jsx || true"
  },
  "author": "Elger van Boxtel",
  "license": "PRIVATE",
  "dependencies": {
    "copy-webpack-plugin": "3.0.1",
    "css-mq packer": "5.0.1",
    "css-mqpacker": "5.0.1",
    "extract-text-webpack-plugin": "1.0.1",
    "postcss-cssnext": "2.7.0",
    "stylelint-webpack-plugin": "0.2.0",
    "webpack": "1.13.1",
    "webpack-error-notification": "0.1.6"
  },
  "devDependencies": {
    "babel-cli": "6.11.4",
    "babel-core": "6.11.4",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.4",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-stage-0": "6.5.0",
    "css-loader": "0.23.1",
    "eslint": "3.1.1",
    "eslint-config-airbnb-base": "5.0.0",
    "eslint-loader": "1.4.1",
    "eslint-plugin-import": "1.12.0",
    "htmlhint-loader": "0.1.0",
    "node-sass": "3.8.0",
    "npm-check": "5.2.2",
    "postcss-loader": "0.9.1",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.0",
    "style-loader": "0.13.1",
    "stylelint": "6.0.1",
    "stylelint-property-unknown": "1.0.1",
    "webpack-dev-server": "1.14.1"
  }
}
