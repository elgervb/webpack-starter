{
  "name": "webpack_dev",
  "version": "0.0.1",
  "description": "Site with webpack live reloading",
  "repository": {
    "type": "git",
    "url": "https://git.lukkien.com/"
  },
  "config": {
    "port": 4000,
    "dest": "./build",
    "src": "./src"
  },
  "scripts": {
    "build": "babel-node -- ./node_modules/.bin/webpack --progress --colors --config webpack/config.build.js",
    "clean": "rimraf $npm_package_config_dest",
    "start": "babel-node -- ./node_modules/webpack-dev-server/bin/webpack-dev-server --inline --hot --colors --content-base=$npm_package_config_src --config ./webpack/config.dev.js --port $npm_package_config_port",
    "start:prod": "babel-node -- ./node_modules/webpack-dev-server/bin/webpack-dev-server --inline --hot --colors --content-base=$npm_package_config_dest --config ./webpack/config.build.js --port $npm_package_config_port",
    "start:remote": "$npm_package_scripts_start --host=0.0.0.0",
    "test:e2e": "./node_modules/.bin/wdio wdio.conf.js",
    "test:e2e:debug": "DEBUG=true $npm_package_scripts_test_e2e",
    "watch": "babel-node -- ./node_modules/.bin/webpack --progress --hot --colors --inline --config webpack/config.dev.js",
    "lint": "eslint . --ext .js --ext .jsx || true",
    "lint:scss": "stylelint 'src/**/*.scss' --syntax scss",
    "webdriver:update": "webdriver-manager update",
    "webdriver:start": "webdriver-manager start",
    "test": "jest --config=jest.json",
    "test:watch": "jest --config=jest.json --watch",
    "test:coverage": "jest --config=jest.json --coverage"
  },
  "author": "Elger van Boxtel",
  "license": "Public",
  "dependencies": {
    "babel-register": "6.18.0",
    "copy-webpack-plugin": "4.0.1",
    "css-mqpacker": "5.0.1",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "2.24.1",
    "karma-coverage": "1.1.1",
    "postcss-cssnext": "2.9.0",
    "stylelint-webpack-plugin": "0.4.0",
    "webdriver-manager": "10.2.10",
    "webdriverio": "4.4.0",
    "webpack": "1.13.3",
    "webpack-error-notification": "0.1.6"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.1",
    "babel-istanbul-loader": "0.1.0",
    "babel-loader": "6.2.8",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-0": "6.16.0",
    "css-loader": "0.26.1",
    "eslint": "3.11.1",
    "eslint-config-airbnb-base": "10.0.1",
    "eslint-loader": "1.6.1",
    "eslint-plugin-import": "2.2.0",
    "htmlhint-loader": "1.0.0",
    "jest": "17.0.3",
    "node-sass": "3.13.0",
    "phantomjs-prebuilt": "2.1.13",
    "postcss-loader": "1.2.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "stylelint": "7.6.0",
    "stylelint-property-unknown": "1.0.1",
    "wdio-jasmine-framework": "0.2.16",
    "webpack-dev-server": "1.16.2"
  }
}
