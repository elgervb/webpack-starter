{
  "name": "webpack_dev",
  "version": "0.0.1",
  "description": "Site with webpack live reloading",
  "repository": {
    "type": "git",
    "url": "https://git.lukkien.com/"
  },
  "config": {
    "port": 4000,
    "dest": "./build",
    "src": "./src"
  },
  "scripts": {
    "build": "babel-node -- ./node_modules/.bin/webpack --progress --colors --config webpack/config.build.js",
    "clean": "rimraf $npm_package_config_dest",
    "start": "babel-node -- ./node_modules/webpack-dev-server/bin/webpack-dev-server --inline --hot --colors --content-base=$npm_package_config_src --config ./webpack/config.dev.js --port $npm_package_config_port",
    "start:prod": "babel-node -- ./node_modules/webpack-dev-server/bin/webpack-dev-server --inline --hot --colors --content-base=$npm_package_config_dest --config ./webpack/config.build.js --port $npm_package_config_port",
    "start:remote": "$npm_package_scripts_start --host=0.0.0.0",
    "test": "karma start",
    "test:watch": "karma start --auto-watch --no-single-run",
    "test:debug": "DEBUG=true $npm_package_scripts_test_watch   ",
    "test:e2e": "./node_modules/.bin/wdio wdio.conf.js",
    "test:e2e:debug": "DEBUG=true $npm_package_scripts_test_e2e",
    "watch": "babel-node -- ./node_modules/.bin/webpack --progress --colors --config webpack/config.dev.js",
    "lint": "eslint . --ext .js --ext .jsx || true",
    "lint:scss": "stylelint 'src/**/*.scss' --syntax scss",
    "webdriver:update": "webdriver-manager update",
    "webdriver:start": "webdriver-manager start"
  },
  "author": "Elger van Boxtel",
  "license": "Public",
  "dependencies": {
    "babel-register": "6.14.0",
    "copy-webpack-plugin": "3.0.1",
    "css-mqpacker": "5.0.1",
    "extract-text-webpack-plugin": "1.0.1",
    "karma-coverage": "1.1.1",
    "postcss-cssnext": "2.8.0",
    "stylelint-webpack-plugin": "0.4.0",
    "webdriver-manager": "10.2.3",
    "webdriverio": "4.2.16",
    "webpack": "1.13.2",
    "webpack-error-notification": "0.1.6"
  },
  "devDependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.0",
    "babel-istanbul-loader": "0.1.0",
    "babel-loader": "6.2.5",
    "babel-polyfill": "6.13.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-stage-0": "6.5.0",
    "css-loader": "0.25.0",
    "eslint": "3.5.0",
    "eslint-config-airbnb-base": "7.1.0",
    "eslint-loader": "1.5.0",
    "eslint-plugin-import": "1.15.0",
    "htmlhint-loader": "1.0.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.7.0",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-firefox-launcher": "1.0.0",
    "karma-ie-launcher": "1.0.0",
    "karma-jasmine": "1.0.2",
    "karma-opera-launcher": "1.0.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "1.8.0",
    "node-sass": "3.10.0",
    "npm-check": "5.2.3",
    "phantomjs-prebuilt": "2.1.12",
    "postcss-loader": "0.13.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "stylelint": "7.6.0",
    "stylelint-property-unknown": "1.0.1",
    "wdio-jasmine-framework": "0.2.6",
    "webpack-dev-server": "1.16.1"
  }
}
